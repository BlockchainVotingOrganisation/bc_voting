{namespace bvs=Goettertz\BcVoting\ViewHelpers}

<label for="name">
	<f:translate key="tx_bcvoting_domain_model_project.name" /> <span class="required">(required)</span>
</label><br />
	<f:form.textfield property="name" /><br />
<label for="description">
	<f:translate key="tx_bcvoting_domain_model_project.description" />
</label><br />
	<f:form.textfield property="description" /><br />
<label for="start">
	<f:translate key="tx_bcvoting_domain_model_project.start" /> <span class="required">(required)</span>
</label><br />
	<f:form.textfield property="start" /><br />
<label for="end">
	<f:translate key="tx_bcvoting_domain_model_project.end" />
</label><br />
	<f:form.textfield property="end" /><br />
<br />
<table>
	<f:for each="{project.assignments}" as="assignment">
		<tr>
			<td>
				{assignment.user}
			</td>
			<td>
				{assignment.role.name}
			</td>
		</tr>
	</f:for>
</table>
